public with sharing class TriangleBad {
    
    private Decimal height;
    private Decimal base;

    public TriangleBad(Decimal height, Decimal base ){
        this.height = height;
        this.base = base;
    }

    public Decimal getHeight(){
        return this.height;
    }

    public Decimal getBase(){
        return this.getBase();
    }

}